{
  "extends": "../../../tsconfig.base.json",
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitOverride": true,
    "noUncheckedIndexedAccess": true,
    "verbatimModuleSyntax": true,
    "noPropertyAccessFromIndexSignature": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "baseUrl": "..",
    "paths": {
      "@onecoach/ui": ["./packages/ui/src/index.ts"],
      "@onecoach/ui/*": ["./packages/ui/src/*"],
      "@onecoach/ui/components": ["./packages/ui/src/components/index.ts"],
      "@onecoach/ui/components/*": ["./packages/ui/src/components/*"],
      "@onecoach/hooks": ["./packages/hooks/src/index.ts"],
      "@onecoach/hooks/*": ["./packages/hooks/src/*"],
      "@onecoach/lib-design-system": ["./packages/lib-design-system/src/index.ts"],
      "@onecoach/lib-design-system/*": ["./packages/lib-design-system/src/*"],
      "design-system": ["./packages/design-system/src/index.ts"],
      "design-system/*": ["./packages/design-system/src/*"],
      "hooks": ["./packages/hooks/src/index.ts"],
      "hooks/*": ["./packages/hooks/src/*"],
      "ui": ["./packages/ui/src/index.ts"],
      "ui/*": ["./packages/ui/src/*"],
      "@onecoach/ui-core": ["./packages/oneui-core/src/index.ts"],
      "@onecoach/ui-core/*": ["./packages/oneui-core/src/*"],
      "@onecoach/ui-workout": ["./packages/oneui-workout/src/index.ts"],
      "@onecoach/ui-workout/*": ["./packages/oneui-workout/src/*"],
      "@onecoach/ui-workout/builder": ["./packages/oneui-workout/src/builder/index.ts"],
      "@onecoach/ui-workout/live": ["./packages/oneui-workout/src/live/index.ts"],
      "@onecoach/ui-nutrition": ["./packages/oneui-nutrition/src/index.ts"],
      "@onecoach/ui-nutrition/*": ["./packages/oneui-nutrition/src/*"],
      "@onecoach/ui-flight": ["./packages/oneui-flight/src/index.ts"],
      "@onecoach/ui-flight/*": ["./packages/oneui-flight/src/*"],
      "@onecoach/ui-coach": ["./packages/oneui-coach/src/index.ts"],
      "@onecoach/ui-coach/*": ["./packages/oneui-coach/src/*"],
      
      "@onecoach/types": ["../onecoach-types/src/index.ts"],
      "@onecoach/types/*": ["../onecoach-types/src/*"],
      "@onecoach/types/client": ["../onecoach-types/src/index.client.ts"],
      
      "@onecoach/features": ["../features/index.ts"],
      "@onecoach/features/*": ["../features/*"],
      "@onecoach/features/workout": ["../features/workout/index.ts"],
      "@onecoach/features/workout/*": ["../features/workout/*"],
      "@onecoach/features/workout/hooks": ["../features/workout/hooks/index.ts"],
      
      "@onecoach/lib-visual-builder": ["../onecoach-backend/packages/lib-visual-builder/src/index.ts"],
      "@onecoach/lib-visual-builder/*": ["../onecoach-backend/packages/lib-visual-builder/src/*"],
      
      "@onecoach/one-workout/utils/progression-calculator": ["../one-workout/src/core/calculators/progression-calculator.ts"],
      "@onecoach/one-workout": ["../one-workout/src/index.ts"],
      
      "@onecoach/lib-workout/intensity-calculator": ["../one-workout/src/core/calculators/intensity-calculator.ts"],
      "@onecoach/lib-workout": ["../one-workout/src/index.ts"],
      "@onecoach/lib-workout/*": ["../one-workout/src/*"]
    }
  }
}
