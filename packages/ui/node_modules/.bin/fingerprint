#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/expo@54.0.25_@babel+core@7.28.5_@expo+metro-runtime@6.1.2_@modelcontextprotocol+sdk@1.2_40569085756de40a800f4094681b4fc9/node_modules/expo/bin/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/expo@54.0.25_@babel+core@7.28.5_@expo+metro-runtime@6.1.2_@modelcontextprotocol+sdk@1.2_40569085756de40a800f4094681b4fc9/node_modules/expo/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/expo@54.0.25_@babel+core@7.28.5_@expo+metro-runtime@6.1.2_@modelcontextprotocol+sdk@1.2_40569085756de40a800f4094681b4fc9/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/expo@54.0.25_@babel+core@7.28.5_@expo+metro-runtime@6.1.2_@modelcontextprotocol+sdk@1.2_40569085756de40a800f4094681b4fc9/node_modules/expo/bin/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/expo@54.0.25_@babel+core@7.28.5_@expo+metro-runtime@6.1.2_@modelcontextprotocol+sdk@1.2_40569085756de40a800f4094681b4fc9/node_modules/expo/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/expo@54.0.25_@babel+core@7.28.5_@expo+metro-runtime@6.1.2_@modelcontextprotocol+sdk@1.2_40569085756de40a800f4094681b4fc9/node_modules:/Users/giulioleone/Sviluppo/CoachOne/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/expo@54.0.25_@babel+core@7.28.5_@expo+metro-runtime@6.1.2_@modelcontextprotocol+sdk@1.2_40569085756de40a800f4094681b4fc9/node_modules/expo/bin/fingerprint" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/expo@54.0.25_@babel+core@7.28.5_@expo+metro-runtime@6.1.2_@modelcontextprotocol+sdk@1.2_40569085756de40a800f4094681b4fc9/node_modules/expo/bin/fingerprint" "$@"
fi
